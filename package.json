{
    "name": "pastanaga-angular",
    "version": "1.0.1",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "build_lib": "ng build --prod pastanaga-angular",
        "postbuild_lib": "cp -R ./projects/pastanaga/src/lib/styles dist/pastanaga/lib/styles && cp -R ./projects/pastanaga/src/assets dist/pastanaga/lib/assets",
        "build_demo": "ng build --prod --base-href /pastanaga-angular/v1/",
        "test": "jest --no-cache",
        "test:watch": "jest --watch",
        "test:ci": "jest --runInBand --ci",
        "get_version": "cat ./projects/pastanaga/package.json | grep version  | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]'",
        "lint": "ng lint"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~10.0.1",
        "@angular/cdk": "~10.0.0",
        "@angular/common": "~10.0.1",
        "@angular/compiler": "~10.0.1",
        "@angular/core": "~10.0.1",
        "@angular/forms": "~10.0.1",
        "@angular/platform-browser": "~10.0.1",
        "@angular/platform-browser-dynamic": "~10.0.1",
        "angular-svg-icon": "^9.0.0",
        "angular-traversal": "^1.6.5",
        "date-fns": "^2.9.0",
        "rxjs": "~6.5.4",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1000.0",
        "@angular-devkit/build-ng-packagr": "~0.1000.0",
        "@angular/cli": "~10.0.0",
        "@angular/compiler-cli": "~10.0.1",
        "@angular/language-service": "~10.0.1",
        "@ngrx/store-devtools": "^8.1.0",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/jest": "24.0.21",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.0",
        "jest": "24.9.0",
        "jest-preset-angular": "^8.2.0",
        "mrs-developer": "^1.1.3",
        "ng-packagr": "^10.0.0",
        "ts-node": "~7.0.0",
        "tslint": "~6.1.0",
        "typescript": "~3.9.5"
    },
    "jest": {
        "preset": "jest-preset-angular",
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupJest.ts"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/"
        ],
        "moduleNameMapper": {
            "pastanaga-angular": "<rootDir>/projects/pastanaga/src"
        }
    }
}
